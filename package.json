{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node build/src/index.js",
    "test": "jest",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json && cp ./src/schema/schema.graphql ./build/src/schema",
    "codegen": "graphql-codegen --config codegen.yml",
    "lint": "eslint . --ext js,ts",
    "lint:fix": "eslint . --ext js,ts --fix",
    "format": "prettier --write '**/*.{ts,js}'",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "postinstall": "npm run db:generate",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SENG499-S22-Company3/backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SENG499-S22-Company3/backend/issues"
  },
  "homepage": "https://github.com/SENG499-S22-Company3/backend#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-resolvers": "2.6.4",
    "@types/bcrypt": "^5.0.0",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.5.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.5",
    "eslint": "^8.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "prettier": "^2.7.0",
    "prisma": "^3.15.1",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "apollo-server-core": "^3.8.1",
    "apollo-server-express": "^3.8.1",
    "bcrypt": "^5.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "graphql": "^16.5.0"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  }
}
